#(entry) [
	out^"Sum of the series with given precision.",$;
	i % 1;
	csum % 0;
	an % 1;
	limit % 0.0001;
	#while (an > limit)[
		an % countCurrent^;
		csum % an+csum;
		out^(round^i,0)," --> ",an,$;
		i % i+1;
	]

	out^"Sum of ",(round^i-1,0)," elements = ",csum;
]

#(countCurrent)[
    return^((1/(2**i))+(1/(3**i)));
]